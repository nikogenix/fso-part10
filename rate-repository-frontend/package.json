	"dependencies": {
		"@apollo/client": "^3.7.14",
		"@expo/metro-config": "^0.7.1",
		"@expo/webpack-config": "^18.0.1",
		"@react-native-async-storage/async-storage": "1.17.11",
		"dotenv": "^16.0.3",
		"expo": "^48.0.17",
		"expo-linking": "^4.0.1",
		"expo-status-bar": "~1.4.0",
		"formik": "^2.4.0",
		"graphql": "^15.8.0",
