		"react-devtools-core": "4.22.0",
		"react-dom": "18.2.0",
		"react-native": "0.71.8",
		"react-native-web": "~0.18.7",
		"react-router-native": "^6.11.2",
		"yup": "^1.2.0"
	},
	"devDependencies": {
		"@babel/core": "^7.12.9",
